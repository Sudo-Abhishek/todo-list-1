{
  "name": "todo-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.3",
    "@mui/material": "^6.4.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "husky": "^9.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "react-scripts": "5.0.1",
    "typescript": "^5.7.3",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*\"",
    "build": "webpack build --config config/webpack.prod.js",
    "lint:fix": "eslint \"src/**/*\"",
    "lint-staged-files": "lint-staged --allow-empty --concurrent false",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev": "webpack serve --config config/webpack.dev.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prepare": "husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ]
      }
    ],
    "ignorePatterns": [
      "package.json",
      "src/**/*.test.ts?(x)",
      "src/**/*.spec.ts?(x)",
      "node_modules"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "eslint-plugin-jest": "^28.11.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "3.4.2",
    "sass": "^1.84.0",
    "sass-loader": "^16.0.4",
    "svg-url-loader": "^8.0.0",
    "ts-loader": "^9.5.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  }
}
